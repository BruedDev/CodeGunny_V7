	<customLanguage>
		<custom stylename="ok" value="O K"/>
		<custom stylename="cancel" value="Hủy bỏ"/>
		<custom stylename="close" value="Đóng"/>
		<custom stylename="times.CurrentDateText" value="Năm {0}tháng{1}ngày{2}"/>
		<custom stylename="times.NextEditionDateText" value="ản tin kế ra mắt:Năm {0}tháng{1}ngày{2}"/>
		<custom stylename="times.CurrentEditionText" value="Kỳ thứ {0}"/>
